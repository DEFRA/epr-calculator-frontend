@{
    ViewData["Title"] = "Run the service administrator calculator - Upload CSV Error";
    var errors = ViewBag.Errors;
}

<div class="govuk-error-summary" data-module="govuk-error-summary">
    <div role="alert">
        <h2 class="govuk-error-summary__title">
            There is a problem
        </h2>
        <div class="govuk-error-summary__body">
            <ul class="govuk-list govuk-error-summary__list">
                @foreach (var error in errors)
                {
                    <li>
                        <a href="@error.DOMElementId">@error.ErrorMessage</a>
                    </li>
                }
            </ul>
        </div>
    </div>
</div>
<div>
    <h1 class="govuk-heading-l">Upload the file again</h1>
    <p class="govuk-body">Make sure you're using the latest version of the <a href="">default parameter spreadsheet template</a></p>
    <p class="govuk-body">The file must be:</p>
    <ul class="govuk-list govuk-list--bullet">
        <li>small than 50KB</li>
        <li>in CSV format</li>
    </ul>
    <p class="govuk-body">It may take a few minutes for your file to upload - do not close the window while the file is uploading.</p>
    <div class="govuk-form-group">
        <input class="govuk-file-upload" id="file-upload-1" name="fileUpload1" type="file">
    </div>
    <button type="submit" class="govuk-button" data-module="govuk-button">
        Upload file
    </button>
</div>