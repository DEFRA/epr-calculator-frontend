@using EPR.Calculator.Frontend.ViewModels
@model CalculatorRunStatusUpdateDto
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var BackLink = "Dashboard";
}
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<partial name="_BackLinkPartial" model="BackLink" />

<main class="govuk-main-wrapper govuk-!-padding-top-4" id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <br>
            <h1 class="govuk-heading-xl">You have successfully deleted this file</h1>
            <span class="govuk-caption-xl">Test data check 2</span><br>
            <p>ID: @Model.RunId</p>
            <hr>
            <br>
            <form action="/form-handler" method="post" novalidate="">
                <input type="hidden" name="answers-checked" value="true">
                <div class="govuk-button-group">
                    <a href="@Url.Action("Index", "Dashboard")" class="govuk-button" type="submit">Return to the dashboard</a>
                </div>
            </form>
        </div>
    </div>
</main>