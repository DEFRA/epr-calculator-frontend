@model CalculatorRunDetailsNewViewModel
@{
    ViewData["Title"] = "Confirmation Success";
}
<main class="govuk-main-wrapper govuk-!-padding-top-4" id="main-content" role="main">
    <partial name="_CurrentUserPartial" model="@Model.CurrentUser" />
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <div class="govuk-panel govuk-panel--confirmation">
                <h1 class="govuk-panel__title">
                    You have deleted calculation run @Model.CalculatorRunDetails.RunName
                </h1>
            </div>
            <br />
            <a href="@Url.Action(ActionNames.Index,ControllerNames.Dashboard)" class="govuk-link">@ActionLinkLabels.ReturnToDashBoard</a>
        </div>
    </div>
</main>