@using EPR.Calculator.Frontend.ViewModels
@using EPR.Calculator.Frontend.Constants

@model string
@{
    ViewData["Title"] = "Confirmation Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<main class="govuk-main-wrapper govuk-!-padding-top-4" id="main-content" role="main">
    <partial name="_CurrentUserPartial" model="(Model)" />
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <div class="govuk-panel govuk-panel--confirmation">
                <h1 class="govuk-panel__title">
                    You have deleted this calculation run
                </h1>                
            </div>
            <br>
            <a href="@Url.Action(ActionNames.Index,ControllerNames.Dashboard)" class="govuk-link">@ActionLinkLabels.ReturnToDashBoard</a>
        </div>
    </div>
</main>